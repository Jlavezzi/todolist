     <%- include('header') -%>
      <h1> To do list</h1>
      <div class="box" id="heading">
        <h3> <%= listTitle%></h3>
      </div>

        <div class="box">

                  <% for (var i = 0; i < item.length; i++) {%>
                       <div class="item">
                          <input type="checkbox" name="" value="">
                         <p><%= item[i]%></p>

                       </div>
                  <%  } %>

      <form class="item" action="/" method="post">
        <input type="text" name="todo" value="" placeholder="Add to list" autocomplete="off">
        <button type="submit" name="list" value=<%=listTitle%>>+</button>

      </form>
        </div>

 <%- include('footer') -%>
